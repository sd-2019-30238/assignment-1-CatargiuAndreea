package Assignment1;

public class Book {
	private String genre;
	private int releaseDate;
	private String author;
	private String title;
	
	public Book(String genre, int releaseDate, String author, String title){
		this.genre = genre;
		this.releaseDate = releaseDate;
		this.author = author;
		this.title = title;
	}
	
	public String getGenre(){
		return genre;
	}
	
	public int getReleaseDate(){
		return releaseDate;
	}
	
	public String getAuthor(){
		return author;
	}
	
	public String getTitle(){
		return title;
	}
}
