package Model;

public class Staff {
	private int idStaff;
	private String name;
	private String password;
	
	public Staff(int idStaff, String name, String password){
		this.idStaff = idStaff;
		this.name = name;
		this.password = password;
	}
	
	public int getId(){
		return idStaff;
	}
	
	public String getName(){
		return name;
	}
	
	public String getPassword(){
		return password;
	}
}
